{
    "name": "laravel-zero/laravel-zero",
    "description": "The Laravel Zero Framework.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/brand7n/picohp/issues",
        "source": "https://github.com/brand7n/picohp"
    },
    "authors": [
        {
            "name": "Brandin L Claar",
            "email": "brandin@remodulate.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "laravel-zero/framework": "^11.0.2",
        "nikic/php-parser": "^5.3",
        "phpstan/phpdoc-parser": "^2.0",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^3.0",
        "laravel/pint": "^1.18.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "phpstan": "php -d memory_limit=-1 vendor/bin/phpstan analyse --configuration phpstan.neon --debug",
        "pest": "vendor/bin/pest",
        "check": [
            "@phpstan",
            "@pest",
            "vendor/bin/pint --test"
        ],
        "all": [
            "@phpstan",
            "@pest",
            "vendor/bin/pint"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["picohp"],
    "extra": {
        "hooks": {
            "config": {
                "stop-on-failure": ["pre-commit"]
            },
            "pre-commit": [
                "git stash -k",
                "composer run-script check"
            ]
        }
    }
}
